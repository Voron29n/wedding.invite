{
  "name": "wedding.invite",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "precommit": "lint-staged",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -r tsconfig-paths/register ./src/index.ts --ignore 'requests/*' \"",
    "prod": "npx tsc && node -r tsconfig-paths/register -r module-alias/register dist/index.js",
    "migrateUp": "npx tsc && npx mikro-orm migration:up",
    "migrateDown": "npx tsc && npx mikro-orm migration:down"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@mikro-orm/migrations": "^5.9.0",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.18",
    "@types/joi": "^17.2.3",
    "@types/node": "^18.18.4",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.9.0",
    "@mikro-orm/core": "^5.9.7",
    "@mikro-orm/mongodb": "^5.9.7",
    "@types/jsonwebtoken": "^9.0.5",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "joi": "^17.11.0",
    "joi-extract-type": "^15.0.8",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "_moduleAliases": {
    "@src": "dist",
    "@const": "dist/const",
    "@controller": "dist/controller",
    "@configuration": "dist/configuration",
    "@entities": "dist/entities",
    "@errors": "dist/errors",
    "@middleware": "dist/middleware",
    "@migration": "dist/migration",
    "@service": "dist/service",
    "@types": "dist/types",
    "@validation": "dist/validation",
    "@repository": "dist/repository"
  }
}
